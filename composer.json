{
    "name": "hiroto-k/console-wrapper",
    "description": "Wrapper class of symfony/console",
    "version": "1.1.2",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Hiroto Kitazawa",
            "email": "hiro.yo.yo1610@gmail.com",
            "homepage": "https://hiroto-k.net/"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "symfony/console": "^4.1",
        "psr/log": "^1.0",
        "hiroto-k/string-builder": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1",
        "friendsofphp/php-cs-fixer": "^2.12",
        "php-coveralls/php-coveralls": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "HirotoK\\ConsoleWrapper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HirotoK\\ConsoleWrapper\\Tests\\": "tests/"
        }
    }
