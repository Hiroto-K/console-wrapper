{
    "name": "hiroto-k/console-wrapper",
    "description": "Wrapper class of symfony/console",
    "version": "1.2.0",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Hiroto Kitazawa",
            "email": "hiroto.ktzw@gmail.com",
            "homepage": "https://hiroto-k.net/"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "symfony/console": "^4.1",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1",
        "friendsofphp/php-cs-fixer": "^2.14"
    },
    "autoload": {
        "psr-4": {
            "HirotoK\\ConsoleWrapper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HirotoK\\ConsoleWrapper\\Tests\\": "tests/"
        }
    }
}
